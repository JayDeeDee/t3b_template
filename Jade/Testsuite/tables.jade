h2 Tables
h3 Bordered Table w/ odd &amp; even - (.contenttable-1)
table(class="contenttable-1")
	thead
		tr
			th(class="td-0") header 1
			th header 2
			th header 3

	tbody
		tr(class="tr-even")
			td(class="td-0")
				img(src="Testsuite/assets/image2.jpg", alt="city-q-g-200-200-2", style="width: 100px;", width="200", height="200")
			td 1.2
			td 1.3

		tr(class="tr-odd")
			td(class="td-0") 2.1
			td 2.2
			td 2.3

		tr(class="tr-even")
			td(class="td-0") 3.1
			td 3.2
			td 3.3

		tr(class="tr-odd tr-last")
			td(class="td-0") 4.1
			td 4.2
			td 4.3


h3 Clean Table - (.contenttable-2)
table(class="contenttable-2")
	caption Caption
	thead
		tr
			th header 1
			th header 2
			th header 3

	tbody
		tr
			td 1.1
			td 1.2
			td 1.3
		tr
			td 2.1
			td 2.2
			td 2.3

	tfoot
		tr
			td tfoot
			td 1
			td 2


h3 Clean Table (condensed) - (.contenttable-3)
table(class="contenttable-3")
	thead
		tr
			th header 1
			th header 2
			th header 3

	tbody
		tr
			td 1.1
			td 1.2
			td 1.3

		tr
			td 2.1
			td 2.2
			td 2.3
